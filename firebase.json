{
  "hosting": {
    "public": "/dist/apps/frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000,
      "source": "/apps/frontend"
    },
    "ui": {
      "enabled": false
    },
    "singleProjectMode": true
  }
}
